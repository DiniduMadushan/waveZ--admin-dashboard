<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AddArtistServlet</servlet-name>
        <servlet-class>servlet.AddArtistServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteArtistServlet</servlet-name>
        <servlet-class>servlet.DeleteArtistServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAlbumServlet</servlet-name>
        <servlet-class>servlet.AddAlbumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAlbumServlet</servlet-name>
        <servlet-class>servlet.DeleteAlbumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAlbumServlet</servlet-name>
        <servlet-class>servlet.EditAlbumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMusicServlet</servlet-name>
        <servlet-class>servlet.AddMusicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMusicServlet</servlet-name>
        <servlet-class>servlet.DeleteMusicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditMusicServlet</servlet-name>
        <servlet-class>servlet.EditMusicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddArtistServlet</servlet-name>
        <url-pattern>/addArtist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteArtistServlet</servlet-name>
        <url-pattern>/deleteArtist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAlbumServlet</servlet-name>
        <url-pattern>/addAlbum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAlbumServlet</servlet-name>
        <url-pattern>/deleteAlbum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAlbumServlet</servlet-name>
        <url-pattern>/editAlbum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMusicServlet</servlet-name>
        <url-pattern>/addMusic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMusicServlet</servlet-name>
        <url-pattern>/deleteMusic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditMusicServlet</servlet-name>
        <url-pattern>/editMusic</url-pattern>
    </servlet-mapping>
</web-app>
